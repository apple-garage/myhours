hoursWeekQuery = "SELECT id_week FROM ( " + hoursQuery +") as hwt";
hoursEmpQuery = "SELECT id_employee FROM ( " + hoursQuery +") as het";

this.queryText += " AND wk.id_week IN (" + hoursWeekQuery + ")";
this.queryText += " AND wk.id_employee IN (" + hoursEmpQuery + ")";
*/

if (params["pais"] != 0){
	this.queryText += " AND e.id_country = '" + params["pais"] + "'";
}

if (params["manager"] != 0){
	managerQuery = "SELECT DISTINCT w.id_employee";
	managerQuery += " FROM work w";
	managerQuery += " JOIN employee e ON w.id_employee = e.id";
	managerQuery += " JOIN assignment a ON w.id_assignment = a.id";
	managerQuery += " WHERE a.id_manager = '" + params["manager"] + "'";
	
	this.queryText += " AND wk.id_employee IN (" + managerQuery + ")";

}

