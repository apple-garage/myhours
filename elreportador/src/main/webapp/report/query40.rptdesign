var finalQueryAdd = "";

//this.queryText += " GROUP BY wk.id_employee, wk.id_week HAVING SUM(wk.hours_x_week) > 40";


 
//	QUERY NUEVA GUACHO
fromQuery = "SELECT e.id AS a1, w.id AS a2";
fromQuery += " FROM work wk, employee e, week w";
fromQuery += " WHERE wk.id_week = w.id";
fromQuery += " AND wk.id_employee = e.id";
fromQuery += " GROUP BY wk.id_employee, wk.id_week";
fromQuery += " HAVING SUM(wk.hours_x_week) <> 40";

//finalQueryAdd = "SELECT wk.*";
finalQueryAdd += /*" FROM work wk*/", (" + fromQuery + ")A";
finalQueryAdd += " WHERE A.a1 = wk.id_employee";
finalQueryAdd += " AND A.a2 = wk.id_week";
finalQueryAdd += " AND wk.id_employee = e.id";	//Relaciones de la query "normal"
finalQueryAdd += " AND wk.id_week = w.id";		//Relaciones de la query "normal"
finalQueryAdd += " AND c.id = e.id_country";	//Relaciones de la query "normal"
finalQueryAdd += " AND m.id = a.id_manager";	//Relaciones de la query "normal"
finalQueryAdd += " AND a.id = wk.id_assignment";	//Relaciones de la query "normal"
//finalQueryAdd += " ORDER BY 2,3";

this.queryText += finalQueryAdd;

// QUERY VIEJITA
/*
SELECT e.id as eid, e.name as ename, w.num_week as wnum, w.end_date as wend, wk.hours_x_week as hours, c.country as cname
FROM work as wk, week as w, employee as e, country as c
WHERE wk.id_employee = e.id
AND wk.id_week = w.id
AND c.id = e.id_country

*/


/*
