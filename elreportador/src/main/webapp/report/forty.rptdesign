            <xml-property name="queryText"><![CDATA[SELECT e.id, e.name, w.start_date, w.end_date, SUM(wk.hours_x_week)
FROM work wk, week w,employee e
WHERE wk.id_employee = e.id
AND wk.id_week = w.id
GROUP BY wk.id_employee, wk.id_week
HAVING SUM(wk.hours_x_week) > 40;
]]></xml-property>
